.myMap
  color red
.mapview
  height 100%
  padding-bottom 40px
.mapdiv
  height 100%
.ng-map-info-window
  background-color #337ab7
  color #337ab7
  div
    &:first-child
      & > div
        &:nth-child(1)
          border-top-color #337ab7 !important
        &:nth-child(3)
          div
            background-color transparent !important
        &:nth-child(4)
          background-color transparent !important
.sitelink
  color #fafafb
  &:hover
    color white
  

